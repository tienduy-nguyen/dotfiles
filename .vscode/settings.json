{
  "workbench.iconTheme": "vscode-icons",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
  "terminal.integrated.env.windows": {
    "CMDER_ROOT": "C:\\Tools"
  },
  "terminal.integrated.shellArgs.windows": [
    "/k",
    "%CMDER_ROOT%\\vendor\\bin\\vscode_init.cmd"
  ],

  "git.confirmSync": false,
  "vsicons.dontShowNewVersionMessage": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "[json]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "window.zoomLevel": -1,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "editor.minimap.enabled": false,
  "[html]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "liveServer.settings.donotVerifyTags": true,
  "files.exclude": {
    "-**/node_modules": true,
    "**/.cache": true,
    "**/.jekyll-cache": true,
    "**/.vscode1": true
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sass]": {
    "editor.defaultFormatter": "michelemelluso.code-beautifier"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.singleQuote": true,
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "editor.formatOnSave": true,
  "go.formatTool": "gofmt",
  "prettier.jsxSingleQuote": true,
  "editor.tabSize": 2,
  "[python]": {
    "editor.tabSize": 4,
    "editor.formatOnPaste": false
  },
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "beautify.config": "",
  "sync.gist": "<my id sync settings for vscode",
  "terminal.integrated.fontFamily": "Menlo for Powerline",
  "python.languageServer": "Pylance",
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "python.formatting.provider": "black",
  "python.formatting.autopep8Args": [
    "--max-line-length=120",
    "--experimental",
    "--indent_size=2"
  ],
  "python.formatting.yapfArgs": ["--style", "{based_on_style: chromium, indent_width: 20}"],
  "python.formatting.blackArgs": ["--line-length=119"],
  "python.sortImports.args": ["-rc", "--atomic"],
  "rufo.exe": "rufo",
  "rufo.useBundler": false,
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave":  true,
  },
  "[typescriptreact]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Dracula",
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "[csharp]": {
    "editor.defaultFormatter": "Leopotam.csharpfixformat"
  },
   // single: Comments are single-line comments that start with three slashes (///) (Default)
    // delimited: Delimited comments that start with a slash and two stars (/**)
    "docomment.syntax": "single",
    // Press the Enter key to activate a command (Default: false)
    "docomment.activateOnEnter": false,
    // User-controllable options
    "docomment.advanced": {
        "cs": {
            "namespace" : {
                "attributes" : ["summary"]
            },
            "class" : {
                "attributes" : ["summary", "typeparam"]
            },
            "interface" : {
                "attributes" : ["summary", "typeparam"]
            },
            "struct" : {
                "attributes" : ["summary"]
            },
            "enum" : {
                "attributes" : ["summary"]
            },
            "delegate" : {
                "attributes" : ["summary", "param", "typeparam", "returns"]
            },
            "field" : {
                "attributes" : ["summary"]
            },
            "property" : {
                "attributes" : ["summary", "value"]
            },
            "method" : {
                "attributes" : ["summary", "param", "typeparam", "returns"]
            },
            "event" : {
                "attributes" : ["summary"]
            }
        }
    },
    "debug.javascript.autoAttachFilter": "disabled",
    "debug.javascript.unmapMissingSources": true,

}
